{
    "name": "jeandepaula/country-validations",
    "description": "A PHP library for validating personal, corporate, banking, and vehicle data for Brazil, USA, and Canada.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jean de Paula",
            "email": "jean.depaula@live.com"
        }
    ],
    "keywords": ["validation", "country", "cpf", "cnpj", "sin", "ssn", "banking", "vehicle"],
    "support": {
        "issues": "https://github.com/JeandePaula/country-validations/issues",
        "source": "https://github.com/JeandePaula/country-validations"
    },
    "require": {
        "php": "^7.4 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "CountryValidations\\Brazil\\": "src/Brazil/",
            "CountryValidations\\Usa\\": "src/Usa/",
            "CountryValidations\\Canada\\": "src/Canada/",
            "CountryValidations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
